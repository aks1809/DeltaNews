                    <div class="card-body">
                        <div class="author d-flex flex-column flex-md-row justify-content-between align-items-center">
                            <span><i class="p-1 fas fa-calendar-day"></i>&nbsp;<%= headlines[7].publishedAt.substring(0,10) %></span>
                            <span class="text-uppercase"><%= headlines[7].author %></span>
                        </div>
                        <h5 class="card-title mt-2"><strong><%= headlines[7].title %></strong></h5>
                    </div>
                </div>
            </div>
